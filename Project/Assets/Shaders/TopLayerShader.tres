render_mode blend_mix;

uniform float CoverStrenght = 0.30000001192093;
uniform sampler2D CoverTexture : repeat_enable;
uniform sampler2D Distortion : repeat_enable;
uniform float DistortionStrenght = 0.00999999977648;
uniform sampler2D screen_tex_frg_2 : hint_screen_texture;



void fragment() {
// FloatParameter:20
	float n_out20p0 = CoverStrenght;


	vec4 n_out18p0;
// Texture2D:18
	n_out18p0 = texture(CoverTexture, UV);


// VectorOp:21
	vec3 n_out21p0 = vec3(n_out20p0) * vec3(n_out18p0.xyz);


// Input:5
	vec2 n_out5p0 = SCREEN_UV;


// Input:11
	float n_out11p0 = TIME;


// FloatOp:15
	float n_in15p1 = 0.00500;
	float n_out15p0 = n_out11p0 * n_in15p1;


// VectorCompose:14
	vec2 n_out14p0 = vec2(n_out15p0, n_out15p0);


// UVFunc:12
	vec2 n_in12p1 = vec2(1.00000, 1.00000);
	vec2 n_out12p0 = n_out14p0 * n_in12p1 + n_out5p0;


	vec4 n_out3p0;
// Texture2D:3
	n_out3p0 = texture(Distortion, n_out12p0);


// FloatParameter:6
	float n_out6p0 = DistortionStrenght;


// VectorOp:7
	vec3 n_out7p0 = vec3(n_out3p0.xyz) * vec3(n_out6p0);


// VectorOp:9
	vec2 n_out9p0 = n_out5p0 + vec2(n_out7p0.xy);


// VectorOp:8
	vec2 n_in8p1 = vec2(0.50000, 0.50000);
	vec2 n_out8p0 = vec2(n_out6p0) * n_in8p1;


// VectorOp:10
	vec2 n_out10p0 = n_out9p0 - n_out8p0;


	vec4 n_out2p0;
// Texture2D:2
	n_out2p0 = texture(screen_tex_frg_2, n_out10p0);


// VectorOp:19
	vec3 n_out19p0 = n_out21p0 + vec3(n_out2p0.xyz);


// Output:0
	COLOR.rgb = n_out19p0;


}
